

from turtle import *

color("black")
pensize(2)
speed(0)

forward (300) 
right(90)     
forward (200)
right(90)     
forward (300)
right(90)     
forward (200)
right(90)

color("black","red")
right(45)
begin_fill() 
forward(141)
left(-135)
forward(100)
right(90)
end_fill()
begin_fill()
right(135)
forward(141)
left(-90)
left(45)
left(-90)
forward(100)
end_fill()
left(-90)
color("black")
forward(200)
