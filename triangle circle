t = turtle.Turtle()
t.speed(0)
for c in ['blue' ,'blue' ,'blue' ,'blue' ,'blue' ,'blue' , 'blue' ,'blue' ,'blue' , 'blue' , ]:
  for c in ['blue' ,'blue' ,'blue' ]:
    for c in ['blue' ,'red' ,'pink' ,'yellow' ,'green']:
        t.color(c)
        t.forward(60)
        t.left(120)
    t.forward(60)

  t.left(10)
